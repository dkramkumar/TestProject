<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Demo QA">
	<listeners>
		<listener class-name="com.demoQA.utility.DemoQAListener"></listener>
	</listeners>

	<test name="Elements">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.demoQA.TestCases.ElementsTest" />
		</classes>
	</test>

	<test name="Forms">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.demoQA.TestCases.FormsTest" />
		</classes>
	</test>

	<test name="AlertsFramesWindows">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.demoQA.TestCases.AlertsFramesWindowsTest" />
		</classes>
	</test>
</suite>
